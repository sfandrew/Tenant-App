<div class="form-group">
 	<%= f.label field.id, field.name.humanize, :class => "col-sm-2 control-label" %>
<div class="col-sm-10">
  	<%= f.select field.id, options_for_select(field.content_meta.split(",")),{},{:class => "form-select form-control", :style => "width: 154px;"} %>
  	<% if defined?(errors) and errors.present? %>
		<small style="color:red"><%= errors.join(', ') %></small>
	<% end %>
  </div>
</div>
