<h1>Listing contactables</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contactables.each do |contactable| %>
      <tr>
        <td><%= link_to 'Show', contactable %></td>
        <td><%= link_to 'Edit', edit_contactable_path(contactable) %></td>
        <td><%= link_to 'Destroy', contactable, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contactable', new_contactable_path %>
